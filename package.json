{
  "name": "rabotyaga-vkma-backend",
  "scripts": {
    "clear:all": "rm -rf ./dist && rm -rf ./node_modules",
    "clear:build": "rm -rf ./dist",
    "clear:deps": "rm -rf ./node_modules",
    "start": "node ./dist/index.js",
    "server": "cross-env NODE_ENV=dev ts-node-dev --respawn --watch --no-notify -r tsconfig-paths/register src/index.ts",
    "build": "cross-env NODE_ENV=prod ttsc -p tsconfig.json",
    "prebuild": "npm run clear:build",
    "prettier": "prettier --write ./**/*"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "dotenv": "^10.0.0",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "mongoose": "^5.12.10",
    "node-cron": "^3.0.0",
    "node-fetch": "^2.6.1",
    "querystring": "^0.2.1",
    "uWebSockets.js": "uNetworking/uWebSockets.js#v20.10.0",
    "vk-io": "^4.2.0"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/kill-port": "^2.0.0",
    "@types/node": "latest",
    "@types/node-cron": "^2.0.3",
    "@types/node-fetch": "^2.5.10",
    "prettier": "^2.7.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "ttypescript": "^1.5.13",
    "typescript": "^4.2.4",
    "typescript-transform-paths": "^3.4.4"
  }
}
